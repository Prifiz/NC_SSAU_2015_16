<!DOCTYPE any PUBLIC '-//Hibernate/Hibernate Mapping DTD 3.0//EN' 'null'>

<any>
    <hibernate-mapping>
        <class name="team5.datamodel.user.newmodel.Address" table="addresses">
            <property column="login" name="login" type="java.lang.String"/>
            <property column="id_city" name="id_city" type="java.lang.integer"/>
            <set name="cities" lazy="false">
                <key column="id_city"/>
                <one-to-one column="id_city" name="city" />
            </set>
        </class>
    </hibernate-mapping>
</any>
