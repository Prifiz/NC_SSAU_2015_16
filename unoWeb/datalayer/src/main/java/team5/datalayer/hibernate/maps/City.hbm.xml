<!DOCTYPE any PUBLIC '-//Hibernate/Hibernate Mapping DTD 3.0//EN' 'null'>

<any>
    <hibernate-mapping>
        <class name="team5.datamodel.user.newmodel.City" table="city">
            <id column="id_city" name="id_city" type="java.lang.integer">
                <generator class=""/>
            </id>
            <property column="city_name" name="cityName" type="java.lang.String"/>
            <property column="id_country" name="id_country" type="java.lang.integer"/>
            <set name="countries" lazy="false">
                <key column="id_country"/>
                <one-to-one column="id_country" name="country" />
            </set>
        </class>
    </hibernate-mapping>
</any>
