<!DOCTYPE any PUBLIC '-//Hibernate/Hibernate Mapping DTD 3.0//EN' 'null'>

<any>
    <hibernate-mapping>
        <class name="team5.datamodel.user.newmodel.ServiceInfo" table="service_info">
            <property column="login" name="login" type="java.lang.String"/>
            <property column="password" name="name" type="java.lang.String"/>
            <property column="data_of_registration" name="dateOfRegistration" type="date"/>
            <one-to-one column="login" name="private_information" />
            <one-to-one column="login" name="addresses" />
        </class>
    </hibernate-mapping>
</any>
