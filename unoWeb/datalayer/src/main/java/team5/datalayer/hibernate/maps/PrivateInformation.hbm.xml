<!DOCTYPE any PUBLIC '-//Hibernate/Hibernate Mapping DTD 3.0//EN' 'null'>
<any>
    <hibernate-mapping>
        <class name="team5.datamodel.user.newmodel.PrivateInformation" table="private_information">
            <property column="login" name="login" type="java.lang.String"/>
            <property column="name" name="name" type="java.lang.String"/>
            <property column="surname" name="surname" type="java.lang.String"/>
        
            <one-to-one column="login" name="service_info" />
            <one-to-one column="login" name="addresses" />
        </class>
    </hibernate-mapping>
</any>